html
	head
		meta(http-equiv="Content-Type", content="text/html; charset=utf-8;")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		title EventMost - #{title}
		link(href="/favicon.ico", type="image/x-icon", rel="icon")
		link(href="/favicon.ico", type="image/x-icon", rel="shortcut icon")

		block css
			link(rel="stylesheet", href="/bootstrap/css/bootstrap.css")
			link(rel="stylesheet", href="/bootstrap/css/bootstrap-responsive.css")
			link(rel="stylesheet", href="/css/flat-ui.css")
			link(rel="stylesheet", href="/css/newdesign.css")
			link(rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Raleway")


	body
		block container
			.row-fluid
				header.span12
					.container
						.row-fluid.headerSpacer
							if everyauth.loggedIn != true
								//- Only displayed when users are guests
								.logo.span3
									a(href="/")
										img(src="/img/logo.png", class="logo")
								.signin.span3.offset6
									a(href="/", class="link1") Login 
									| |
									a(href="", class="link1")  Sign up
							else
								//- Only displayed when user is logged in
								.logo.span3
									a(href="/")
										img(src="/img/logo.png", class="logo")
								.menu.span7.right
									a(href="/", class="link1") Home 
									| |
									a(href="/", class="link1")  Create event
								.span7.headerspacer2
									form(method="POST", accept-charset="utf-8")
										.right
											input.gonew2(type="button", value="GO")
										input(type="hidden", name="_csrf", value=token)
										input(type="search", placeholder="search events of people", class="file_input_textbox4")
								img(src="/img/dot.svg", class="purpleline")
								.right.minus2
									.right.image
										img(src="/img/messages.svg") 
										h5 Inbox
										h15 3

