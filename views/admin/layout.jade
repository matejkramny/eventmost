html
	head
		meta(http-equiv="Content-Type", content="text/html; charset=utf-8;")
		meta(name="viewport", content="width=device-width, initial-scale=1")
		title EventMost Admin
		link(href="/favicon.ico", type="image/x-icon", rel="icon")
		link(href="/favicon.ico", type="image/x-icon", rel="shortcut icon")
		block css
			link(href="//netdna.bootstrapcdn.com/bootswatch/3.0.1/flatly/bootstrap.min.css", rel="stylesheet")
			link(href="//netdna.bootstrapcdn.com/font-awesome/4.0.2/css/font-awesome.min.css", rel="stylesheet")
			style.
				.container-full {
					margin-left: auto;
					margin-right: auto;
					padding-left: 10px;
					padding-right: 10px;
					width: 100%
				}
				html,body {
					margin: 0;
					padding: 0;
					border: 0;
					width: -webkit-calc(100% - 10px);
					width: -moz-calc(100% - 10px);
					width: calc(100% - 10px);
				}
